<script>

	let loader = document.getElementById('loader');
	let section = document.getElementsByClassName('section_principal');

	for(let i=0; i<section.length; i++){
		console.log(section[i]);
	}

	let logout = document.getElementById('logout');

	logout.addEventListener('click', e => {

		for(let i=0; i<section.length; i++){
			section[i].style.display='none';
		}

		loader.style.display='block';
	});


	let bouttonUpdate = document.getElementById('bouttonUpdate');
	let bouttonDelete = document.getElementById('bouttonDelete');

	bouttonUpdate.addEventListener('click', e => {
		for(let i=0; i<section.length; i++){
			section[i].style.display='none';
		}
		loader.style.display='block';
	});

	bouttonDelete.addEventListener('click', e => {
		for(let i=0; i<section.length; i++){
			section[i].style.display='none';
		}
		loader.style.display='block';

	});

</script>
